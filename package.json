{
  "name": "moen-server",
  "version": "0.0.1",
  "private": true,
  "description": "This server is for moen api",
  "scripts": {
    "build": "npm run clean && node esbuild.config.js",
    "build:dev": "npm run clean && node esbuild.config.js --watch",
    "build:prod": "NODE_ENV=production npm run build",
    "copy:config": "node scripts/copyConfig.js",
    "clean": "rimraf dist",
    "start": "node ./dist/bin/www",
    "start:dev": "ts-node ./bin/www",
    "start:debug": "node --inspect=9229 -r ts-node/register ./bin/www",
    "start:prod": "node dist/bin/www.js",
    "start:azure": "node dist/bin/www.js",
    "test": "jest --detectOpenHandles",
    "test:all": "ts-node src/tests/run-tests.ts all",
    "test:integration": "ts-node src/tests/run-tests.ts integration",
    "test:routes": "ts-node src/tests/run-tests.ts routes",
    "test:report": "ts-node src/tests/run-tests.ts report",
    "test:pattern": "ts-node src/tests/run-tests.ts pattern",
    "test:file": "ts-node src/tests/run-tests.ts file",
    "test:watch": "jest --watch --detectOpenHandles",
    "test:coverage": "jest --coverage --detectOpenHandles",
    "prettier": "prettier --no-error-on-unmatched-pattern --write --config ./.prettierrc --ignore-path=./.prettierignore \"**/*.js\" \"**/*.jsx\" \"**/*.ts\" \"**/*.tsx\"",
    "prettier:check": "prettier --no-error-on-unmatched-pattern --check --config ./.prettierrc --ignore-path=./.prettierignore \"**/*.js\" \"**/*.jsx\" \"**/*.ts\" \"**/*.tsx\"",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "lint:quiet": "eslint --quiet",
    "inject-secrets": "node scripts/toggleSecrets.js inject",
    "restore-secrets": "node scripts/toggleSecrets.js restore",
    "security:audit": "node scripts/security-audit.js",
    "security:check": "npm audit --audit-level=moderate && npm run security:audit",
    "security:fix": "npm audit fix",
    "security:update": "npm update"
  },
  "dependencies": {
    "@azure/identity": "^4.10.1",
    "@azure/keyvault-secrets": "^4.10.0",
    "@pageproof/sdk": "^2.3.2",
    "archiver": "^7.0.1",
    "async-mutex": "^0.5.0",
    "axios": "^1.10.0",
    "base64-stream": "^1.0.0",
    "busboy": "^1.6.0",
    "compression": "^1.8.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.0",
    "globals": "^16.2.0",
    "helmet": "^8.1.0",
    "http-errors": "~2.0.0",
    "morgan": "~1.10.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "yazl": "^3.3.1",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@types/archiver": "^6.0.3",
    "@types/base64-stream": "^1.0.5",
    "@types/busboy": "^1.5.4",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/debug": "^4.1.12",
    "@types/express": "^5.0.3",
    "@types/http-errors": "^2.0.5",
    "@types/jest": "~30.0.0",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.0.3",
    "@types/supertest": "^6.0.3",
    "@types/yazl": "^3.3.0",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "esbuild": "^0.20.0",
    "esbuild-plugin-copy": "^2.1.1",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.0",
    "husky": "^9.1.7",
    "jest": "30.0.2",
    "jest-junit": "~16.0.0",
    "patch-package": "^8.0.0",
    "rimraf": "^6.0.1",
    "supertest": "^7.1.1",
    "ts-jest": "^29.4.1",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  }
}
